{
  "name": "fountain-parser-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "Frontend React app for Fountain Parser with lexicological processing",
  "type": "module",
  "scripts": {
    "dev": "rm -rf node_modules/.vite && npx vite --host 0.0.0.0 --port 3000 --strictPort",
    "build": "vite build",
    "watch": "vite build --watch",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "@codemirror/commands": "^6.3.3",
    "@codemirror/highlight": "^0.19.8",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/language": "^6.10.1",
    "@codemirror/search": "^6.5.6",
    "@codemirror/state": "^6.4.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.26.3",
    "compromise": "^14.12.0",
    "franc": "^6.1.0",
    "natural": "^6.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "retext": "^9.0.0",
    "retext-keywords": "^8.0.0",
    "retext-pos": "^5.0.0"
